<div class="container">
  <%= form_for @post, :html => {:multipart => true} do |f| %>
    <%= f.label :Title %><br>
    <%= f.text_field :title %><br><br>
    <%= f.label :Body %><br>
    <%= f.text_area :body %><br><br>
    <%= f.submit %>
  <% end %><br><br>
  <%= link_to "HOME", root_path ,class: "btn btn-info" %>
</div>
